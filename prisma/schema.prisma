datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Feedback {
  id         Int      @id @default(autoincrement())
  rating     Int
  review     String
  aiResponse String
  aiSummary  String
  aiAction   String
  createdAt  DateTime @default(now())
}
